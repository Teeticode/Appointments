{
  "expo": {
    "name": "Vee_Nails",
    "slug": "Vee_Nails",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/spash_vee.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F3E9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Vee-Nails"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vee_android.png",
        "backgroundColor": "#F5F3E9"
      },
      "package": "com.anonymous.Vee_Nails",
      "ndk": "25.1.8937393"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24,
            "ndkVersion": "25.1.8937393"
          },
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.4"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
